{
  "name": "spring-react-client",
  "version": "0.0.1",
  "description": "Spring & React Starter Kit - Client",
  "main": "main.js",
  "scripts": {
    "lint": "better-npm-run lint",
    "start": "better-npm-run start",
    "bundle": "better-npm-run bundle"
  },
  "betterScripts": {
    "lint": {
      "command": "eslint src"
    },
    "start": {
      "command": "webpack-dev-server --config webpack/webpack.dev.config.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": "8000",
        "API_PORT": "8080"
      }
    },
    "bundle": {
      "command": "webpack -p --config webpack/webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "API_PORT": "8080"
      }
    }
  },
  "author": "Miroslav Lacina",
  "license": "MIT",
  "dependencies": {
    "formsy-react": "^0.18.0",
    "react": "^15.0.2",
    "react-bootstrap": "^0.29.3",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-bootstrap": "^0.23.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "better-npm-run": "0.0.8",
    "bootstrap-sass": "^3.3.6",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.7.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.2.0",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iviireczech/spring-react.git"
  }
}
